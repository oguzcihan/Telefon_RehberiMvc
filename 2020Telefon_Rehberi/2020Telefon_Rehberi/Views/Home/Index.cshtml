@model List<_2020Telefon_Rehberi.Models.telRehber>

@{
    ViewBag.Title = "Anasayfa";
}

<table class="table">
    <tr>

        <th>
            <b>Ad Soyad</b>
        </th>
        <th>
            <b>Telefon</b>
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.adSoyad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.telefon)
            </td>

        </tr>
    }

</table>

